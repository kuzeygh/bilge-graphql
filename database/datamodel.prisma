type Post {
  id: ID! @unique
  title: String!
  content: String!
  pictures: [PostImage]
  published: Boolean! @default(value: "false")
  author: User
}

type User {
  id: ID! @unique
  name: String!
  email: String! @unique
  password: String!
  posts: [Post!]!
}

type PostImage {
  id: ID! @unique
  pictureURL: String!
}
